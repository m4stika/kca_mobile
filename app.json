{
  "expo": {
    "name": "KCA-Mobile",
    "slug": "KCA-Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "KCA-Mobile",
    "userInterfaceStyle": "automatic",
    "androidStatusBar": {
      "backgroundColor": "#ffffff",
      "translucent": false
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mastika.KCAMobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mastika.KCAMobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "70b0a9aa-71b6-4ba2-ae20-77b4ca6b9af3"
      }
    }
  }
}
