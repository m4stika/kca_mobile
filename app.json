{
  "expo": {
    "name": "KCA-Mobile",
    "slug": "KCA-Mobile",
    "description": "Koperasi KCA",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kca-mobile",
    "userInterfaceStyle": "automatic",
    "androidStatusBar": {
      "backgroundColor": "#ffffff",
      "translucent": false
    },
    "splash": {
      "image": "./assets/images/kca.png",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mastika.KCAMobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/kca.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mastika.KCAMobile",
      "versionCode": 6
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "70b0a9aa-71b6-4ba2-ae20-77b4ca6b9af3"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/70b0a9aa-71b6-4ba2-ae20-77b4ca6b9af3"
    }
  }
}
